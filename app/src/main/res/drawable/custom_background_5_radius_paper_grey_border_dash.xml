<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="true"
        android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="5dp" />
            <stroke android:color="@color/colorGrayApp"
                android:width="@dimen/all_line_split_super_slim"
                android:dashGap="@dimen/all_padding_short"
                android:dashWidth="@dimen/all_padding_short"/>
            <solid android:color="#f7f5ed"/>
        </shape>
    </item>

    <item
        android:state_enabled="true"
        android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="5dp" />
            <stroke android:color="@color/colorGrayApp"
                android:width="@dimen/all_line_split_super_slim"
                android:dashGap="@dimen/all_padding_short"
                android:dashWidth="@dimen/all_padding_short"/>
            <solid android:color="@color/colorPressed"/>
        </shape>
    </item>
</selector>